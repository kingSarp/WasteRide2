{
  "expo": {
    "name": "WasteRide2",
    "slug": "WasteRide2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.salamitech.WasteRide2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(WasteRide2) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "37c0b55e-21b4-4b4a-9b53-9c5c0a2ef8a7"
      },
      "GOOGLE_MAPS_API_KEY": "AIzaSyDL_hAQJdEdP1WmJirJssWtR2NkDiU3FAs"
    }
  }
}
